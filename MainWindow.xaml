<Window x:Class="_185909ISpeakTxtMSG.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_185909ISpeakTxtMSG"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <TextBox x:Name="TXTInput" Text="Enter TXT Form Here."   TextWrapping="Wrap" AcceptsReturn="True" />
        <Button x:Name="TranslateTxt" Click="TranslateTxt_Click" Height="38" Content="Click To Translate"/>
        <Label x:Name="TXTOutput" />
    </StackPanel>
</Window>
